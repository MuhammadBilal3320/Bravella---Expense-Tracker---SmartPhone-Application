{
  "expo": {
    "name": "Bravella",
    "slug": "Bravella-ExpenseTracker",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/icon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.muhammadbilalhaha.myexpoapp"
    },
    "extra": {
      "eas": {
        "projectId": "fc57f540-0271-4173-aff6-285e911eaa99"
      }
    },
    "owner": "muhammadbilalhaha"
  }
}
